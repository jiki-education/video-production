{
  "id": "example-pipeline",
  "title": "Example Pipeline",
  "version": "1.0",
  "config": {
    "storage": {
      "bucket": "jiki-videos",
      "prefix": "lessons/example-pipeline/"
    },
    "workingDirectory": "./output"
  },
  "nodes": [
    {
      "id": "intro_script",
      "type": "asset",
      "inputs": {},
      "config": {},
      "asset": {
        "source": "./scripts/intro.md",
        "type": "text"
      }
    },
    {
      "id": "code_config",
      "type": "asset",
      "inputs": {},
      "config": {},
      "asset": {
        "source": "./code/segment-1.json",
        "type": "json"
      }
    },
    {
      "id": "code_screen",
      "type": "render-code",
      "inputs": {
        "config": "code_config"
      },
      "config": {
        "provider": "remotion",
        "compositionId": "code-scene"
      }
    }
  ]
}
